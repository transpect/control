    <users>
      <user>
        <name>admin</name>
      </user>
    </users>
    <groups>
      <group>
        <name>admin</name>
      </group>
    </groups>
    <rels>
      <rel>
        <group> admin </group>
        <user> admin </user>
      </rel>
    </rels>
    <repos> </repos>
  </access>
  <conversions/>
</management>
